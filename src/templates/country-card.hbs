{{#each this}}
  <div class="card">
    <div class="card-img-top">
      <img src="{{flags.svg}}" alt="flag" width="25px" height="20px">
      <h2 class="card-title">{{name.official}}</h2>
    </div>
    <div class="card-body">
      <p class="card-text"><b>Capital:</b> {{capital}}</p>
      <p class="card-text"><b>Population:</b> {{population}}</p>
      <p class="card-text"><b>languages:</b>
      <ul class="list-group">
      {{#each languages}}
        {{#if }}
          <li class="list-group-item">{{this}},</li>
        {{else}}
          <li class="list-group-item">{{this}}</li>
        {{/if}}
      {{/each}}
      </ul>
      </p>
      
    </div>
  </div>
{{/each}}